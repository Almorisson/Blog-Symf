{% extends 'base.html.twig' %}

{% block title %} MyBlog - HomePage {% endblock %}
{% block body %}
    {% for id, post in posts %}
        <p><a href="{{ path('blog_show', {'id': id }) }}">ID : {{ id }}</a></p>
        <h3>{{ post.title }}</h3>
        <h5>{{ post.text }}</h5>
    {% endfor %}
{% endblock %}
